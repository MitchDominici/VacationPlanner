@model IList<VacationPlanner.Models.Itinerary>
@{
    ViewData [ "Title" ] = "ViewItineraries";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>View Itineraries</h1>


<table class="table">

    

    @foreach ( var itin in Model )
    {
        <tr>
            <th>@itin.Name.Name</th>
        
        </tr>
        <tr>
            <td>@itin.Location</td>
            
        </tr>
    }
</table>

@if ( Model.Count == 0 )
{
    <p>No events yet!</p>
}

