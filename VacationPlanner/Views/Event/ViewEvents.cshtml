@model IList<VacationPlanner.Models.Event>
@{
    ViewData [ "Title" ] = "ViewItineraries";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>View Itineraries</h1>


<table class="table">
    

    @foreach (var itin in Model)
    {
        <tr>
            <th>@itin.City</th>

        </tr>
        <tr>
            <td>@itin.Location</td>

        </tr>
    }
    
</table>

@if (Model.Count == 0)
{
    <p>No events yet</p>
}
<br/>
<div style="padding-bottom:100px">
    <h6> <a asp-controller="Event" asp-action="RemoveEvents" asp-route-id="">Remove Events</a></h6>
</div>